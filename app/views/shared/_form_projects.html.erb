<%= search_form_for @search, url: projects_path, method: :get do |f| %>
  <%= f.label :city, "City" %>
  <%= f.select :city_eq, options_for_select([['Select', ''], ['Paris'], ['Lille'], ['Lyon']], :selected => :city) %>
  <%= f.label :genre %>
  <%= f.search_field :genre%>
  <%= f.label :start_date %>
  <%= f.search_field :start_date%>
  <%= f.label :end_date %>
  <%= f.search_field :end_date%>
  <%= f.submit %>
<% end %>
